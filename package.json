{
  "name": "anywhere",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "commit": "pnpm lint && pnpm format && git add . && pnpm dlx git-cz",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "database:generate": "cd packages/database && npm run generate && cd -",
    "backend": "cd apps/api && pnpm run dev",
    "frontend": "cd apps/host && pnpm run dev"
  },
  "devDependencies": {
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "jotai": "^2.12.5"
  },
  "pnpm": {
    "overrides": {
      "react": "18.0.0",
      "react-dom": "18.0.0",
      "antd": "5.13.0",
      "@ant-design/icons": "6.0.0",
      "@types/react": "18.0.37",
      "@types/react-dom": "18.0.11"
    }
  }
}
