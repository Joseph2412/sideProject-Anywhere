{
  "name": "anywhere",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "commit": "pnpm lint && pnpm format && git add . && pnpm dlx git-cz",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "database:generate": "cd packages/database && npm run generate && cd -"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "jotai": "^2.12.5"
  }
}
